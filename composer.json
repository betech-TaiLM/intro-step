{
    "name": "whtht/intro-step",
    "description": "Laravel intro step plugin with introjs",
    "keywords": ["laravel","intro"],
    "license": "MIT",
    "authors": [
        {
            "name": "Musa Kurt",
            "email": "hackedtim61@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Whtht\\IntroStep\\": "src/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "7.5.x-dev"
    },
    "autoload-dev": {
        "classmap": [
            "tests/TestCase.php"
        ]
    },
    "extra":{
        "laravel":{
            "providers":[
                "Whtht\\IntroStep\\IntroStepServiceProvider"
            ],
            "aliases":{
                "IntroStep": "Whtht\\IntroStep\\Facade\\IntroStep"
            }
        }
    },
    "minimum-stability": "dev"
}
